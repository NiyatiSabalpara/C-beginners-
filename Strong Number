//Strong number-Sum of Factorial of digits of a number is equal to the number
#include<iostream>
using namespace std;
int main(){
    int n, s=0, f=1, i, j, p;
    cout<<"Enter the number:: "<<endl;
    cin>>n;
    while(n>0){
        n=p;
        j=n%10;
        for(i=1; i<=j; i++){
            f=f*i;
            s+=f;
            n/=10;
        }
    }
    if(s==p)
        cout<<"STRONG";

    else
        cout<<"NOT";
    return 0;
}
