#include<iostream>
using namespace std;
//SWITCH CASE
int main(){
    char a;
    cout<<"Enter the character:: "<<endl;
    cin>>a;
    switch(a){
    case 'A':
    case 'E':
    case 'I':
    case 'O':
    case 'U':
    case 'a':
    case 'e':
    case 'i':
    case 'o':
    case 'u':
        cout<<"Vowel";
        break;
    default:
        cout<<"Consonant";
    }
return 0;
}
