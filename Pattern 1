//making patterns using nested loops
/*
A
BC
DEF
GHIJ
KLMNO */
#include<iostream>
using namespace std;
int main(){
   int i, j, n, c=65;
   cout<<"Enter the number of rows:: "<<endl;
   cin>>n;
   for(i=0; i<=n; i++){
    for(j=0; j<=i; j++){
        cout<<(char)c;
        c++;
    }
   cout<<endl;
   }
   return 0;
}
