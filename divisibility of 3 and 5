#include<iostream>
using namespace std;
int main(){
    int n;
    cout<<"Enter the number to be checked::: "<<endl;
    cin>>n;
    if(n%3==0 && n%5==0){
            cout<<"BOTH";
    }
    else if(n%5==0){
        cout<<"FIVE";
    }
    else if (n%3==0){
        cout<<"THREE";
    }
    else
        cout<<"NONE";
    return 0;
}
