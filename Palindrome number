#include<iostream>
using namespace std;
//The reverse of number is same as the original number is palindrome number
int main(){
   int n,n1, n2, i=0;
   cout<<"Enter the number:: "<<endl;
   cin>>n;
   n=n1;
   while(n1>0){
    n2=n1%10;
    i=(i*10)+ n2;
    n1/=10;
   }
   if(n==i)
    cout<<"Palindrome Number.";
   else
    cout<<"Not a Palindrome number.";
   return 0;
}
